    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/example/main.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program2",
            "program": "${workspaceFolder}/example/readdir.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "args": ["${workspaceFolder}/src/index.ts"],
            "runtimeArgs": ["-r", "ts-node/register"],
            "internalConsoleOptions": "openOnSessionStart",
            "protocol": "inspector"
        }
    ]
}